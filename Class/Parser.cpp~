#include		"Parser.hh"

Parser::Parser()
{
}

Parser::~Parser()
{
  while (_map.size())
    _map.pop_back();
}

int				Parser::getWidth()const{return _width;}
int				Parser::getHeight()const{return _height;}
std::vector<int> const		&Parser::getMap()const{return _map;}

void				Parser::pars(std::string const &path)
{
  std::fstream			file;

  file.open(path.c_str(), std::ios_base::in);
  file >> _width;
  std::cout << _width << std::endl;
}
